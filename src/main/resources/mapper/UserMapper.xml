<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.saintdan.framework.mapper.UserMapper">

    <resultMap type="com.saintdan.framework.po.User" id="userMap">
        <result property="id" column="id"/>
        <result property="name" column="name"/>
        <result property="usr" column="usr"/>
        <result property="pwd" column="pwd"/>
        <result property="isAccountNonExpiredAlias" column="is_account_non_expired_alias"/>
        <result property="isAccountNonLockedAlias" column="is_account_non_locked_alias"/>
        <result property="isCredentialsNonExpiredAlias" column="is_credentials_non_expired_alias"/>
        <result property="isEnabledAlias" column="is_enabled_alias"/>
        <result property="status" column="status"/>
        <result property="description" column="description"/>
        <result property="lastLoginAt" column="last_login_at"/>
        <result property="ip" column="ip"/>
        <result property="authorityStr" column="authority_str"/>
        <result property="createdAt" column="created_at"/>
        <result property="createdBy" column="created_by"/>
        <result property="lastModifiedAt" column="last_modified_at"/>
        <result property="lastModifiedBy" column="last_modified_by"/>
        <result property="version" column="version"/>
    </resultMap>

</mapper>